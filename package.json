    "less": "^4.2.0",
    "lodash-es": "^4.17.21",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^2.9.18",
    "vite-plugin-imp": "^2.4.0",
    "vite-plugin-md": "^0.6.7",
